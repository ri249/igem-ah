              const readMore = document.getElementById("read-more");
              const showMore = document.getElementById("show-more");
              if (hidden) {
                readMore.style.opacity = "1";
                readMore.style.height = "auto";
                readMore.style.overflow = "auto";
              } else {
                readMore.style.opacity = "0";
                readMore.style.height = "15px";
                readMore.style.overflow = "hidden";
              }
              readMore.setAttribute("aria-hidden", !hidden);
              hidden = !hidden;
              if (!hidden) {
                showMore.innerText = "read less";
              } else {
                showMore.innerText = "read more";
              }
            }
          </script>

